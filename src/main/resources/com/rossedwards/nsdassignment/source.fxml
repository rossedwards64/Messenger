            <ToggleButton fx:id="subscribeButton2" layoutX="223.0" layoutY="103.0" mnemonicParsing="false"
                          onAction="#subscribe" text="Subscribe"/>
            <ToggleButton fx:id="subscribeButton3" layoutX="223.0" layoutY="152.0" mnemonicParsing="false"
                          onAction="#subscribe" text="Subscribe"/>
            <ToggleButton fx:id="subscribeButton4" layoutX="223.0" layoutY="200.0" mnemonicParsing="false"
                          onAction="#subscribe" text="Subscribe"/>
            <ToggleButton fx:id="subscribeButton5" layoutX="223.0" layoutY="248.0" mnemonicParsing="false"
