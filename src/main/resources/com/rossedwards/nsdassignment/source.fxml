          <Label fx:id="isConnectedLabel" maxHeight="1.7976931348623157E308" onMouseMoved="#connectedState"
                 text="Connected" HBox.hgrow="ALWAYS">
              <font>
                  <Font size="11.0" fx:id="x3"/>
              </font>
              <textFill>
                  <Color red="0.625" green="0.625" blue="0.625" fx:id="x4"/>
              </textFill>
          </Label>
          <Pane HBox.hgrow="ALWAYS"/>
      </HBox>
  </VBox>
</AnchorPane>
