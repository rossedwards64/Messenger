                <font>
                    <Font size="18.0" fx:id="x11"/>
                </font>
                <textFill>
                    <Color red="0.624" green="0.624" blue="0.624" fx:id="x21"/>
                </textFill>
            </Label>
            <Label layoutX="19.0" layoutY="140.0" prefHeight="40.0" prefWidth="66.0" text="Name:">
